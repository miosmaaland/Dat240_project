@page
@model SmaHauJenHoaVij.Pages.AdminPages.SetDeliveryFee.SetDeliveryFeeModel
@{
    ViewData["Title"] = "Set Delivery Fee";
}

<h1>Set Delivery Fee</h1>

<form method="post">
    <div class="form-group">
        <label for="fee">Delivery Fee</label>
        <input type="number" step="0.01" class="form-control" id="fee" name="Fee" required />
    </div>
    <button type="submit" class="btn btn-primary">Set Fee</button>
</form>
